# 数据库配置
DATABASE_URL="postgresql://user:password@localhost:5432/api_proxy"

# Redis 配置
REDIS_URL="redis://localhost:6379"

# NextAuth 配置
NEXTAUTH_SECRET="your-super-secret-key-change-this-in-production"
